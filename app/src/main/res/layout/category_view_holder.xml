<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="76dp"
    android:id="@+id/constraintLayout"
    android:background="@drawable/list_background1"
    android:layout_height="wrap_content"
    android:layout_margin="9dp">

    <ImageView
        android:id="@+id/image_View_pic"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toTopOf="@+id/text_View_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.61"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51"
        app:srcCompat="@drawable/list_1" />

    <TextView
        android:id="@+id/text_View_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:text="Title"
        android:textAlignment="center"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/image_View_pic"
        app:layout_constraintStart_toStartOf="@+id/image_View_pic"
        app:layout_constraintTop_toBottomOf="@+id/image_View_pic" />
</androidx.constraintlayout.widget.ConstraintLayout>